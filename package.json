{
  "name": "eslint-config-barney",
  "version": "2.1.0",
  "description": "Barney Parker's ESLint config",
  "author": "Barney Parker",
  "license": "MIT",
  "main": "index.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/barneyparker/eslint-config-barney.git"
  },
  "keywords": [
    "eslint",
    "lint",
    "jsdoc",
    "esm",
    "es6",
    "node",
    "browser"
  ],
  "bugs": {
    "url": "https://github.com/barneyparker/eslint-config-barney/issues"
  },
  "homepage": "https://github.com/barneyparker/eslint-config-barney#readme",
  "peerDependencies": {
    "eslint": ">= 8"
  },
  "dependencies": {
    "eslint-plugin-jsdoc": "^48.8.3"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "version:patch": "npm version patch --commit-hooks=false",
    "version:minor": "npm version minor --commit-hooks=false",
    "version:major": "npm version major --commit-hooks=false",
    "postversion": "git push && git push --tags && npm publish",
    "doc": "jsdoc2md -t README.hbs index.js > README.md"
  }
}
